     1                          ; MARVIN build for beanzee / Z80 breadboard computer
     2                          ; https://github.com/PainfulDiodes/BeanZee
     3                          
     4                          
     5                          BEANBOARD EQU 0
     6                          
     7                          include "asm/main.inc"
asm/main.inc:
     1                          ; start of user RAM
     2                          RAMSTART equ 0x8000
     3                          
     4                          ; 8-byte status area
     5                          CONSOLE_STATUS equ 0xf000
     6                          ; 8-byte keyscan buffer
     7                          KEY_MATRIX_BUFFER equ 0xf010
     8                          ; command buffer
     9                          CMD_BUFFER equ 0xf020
    10                          ; this should really be 0x0000 as the CPU will dec SP before PUSH
    11                          STACK equ 0xffff
    12                          
    13                          UM245R_CTRL equ 0 ; serial control port
    14                          UM245R_DATA equ 1 ; serial data port
    15                          KEYSCAN_OUT equ 2 ; either 2 or 3 will work
    16                          KEYSCAN_IN  equ 3 ; either 2 or 3 will work
    17                          LCD_CTRL    equ 4 ; LCD control port
    18                          LCD_DATA    equ 5 ; LCD data port
    19                          GPIO_OUT    equ 6 ; either 6 or 7 will work
    20                          GPIO_IN     equ 7 ; either 6 or 7 will work
    21                          
    22                          CONSOLE_STATUS_USB equ 1
    23                          CONSOLE_STATUS_BEANBOARD equ 2
    24                          
beanzee.asm:
     8                          include "asm/escapestring.inc"
asm/escapestring.inc:
     1                          ; escape character constants for assembler compatibility
     2                          ; sjasmplus requires double quotes around escape sequences: "\n" and would not interpret '\n' but truncate
     3                          ; z88dk-z80asm requires single quotes around so would correctly interpret '\n' but reject "\n"
     4                          
     5                          ESC_B equ 0x08 ; \b
     6                          ESC_T equ 0x09 ; \t
     7                          ESC_N equ 0x0a ; \n
     8                          ESC_R equ 0x0d ; \r
     9                          ESC_E equ 0x1b ; \e
    10                          SLASH equ 0x5c ; \\
    11                          QUOTE equ 0x27 ; \'
    12                          
beanzee.asm:
     9                          include "asm/main.asm"
asm/main.asm:
     1                          IFDEF ORGDEF
     2                              ORG ORGDEF
     3                          ELSE
     4                              ORG 0x0000
     5                          ENDIF
     6                          
     7  0000  31ffff                ld sp, STACK
     8                          IF BEANBOARD
     9                              call lcd_init
    10                              call beanboard_console_init
    11                          ENDIF
    12  0003  c36500                jp START
    13                          
beanzee.asm:
    10                          include "asm/console.asm"
asm/console.asm:
     1                          ; wait for a character and return in A
     2                          getchar:
     3  0006  cd0e00                call readchar
     4  0009  fe00                  cp 0
     5  000b  c0                    ret nz
     6  000c  18f8                  jr getchar
     7                          
     8                          IF BEANBOARD
     9                          ; read a character from the console and return in A - return 0 if there is no character
    10                          readchar:
    11                              push hl
    12                              ld hl,CONSOLE_STATUS
    13                              ld a,CONSOLE_STATUS_BEANBOARD
    14                              and (hl)
    15                              jr nz,_readchar_beanboard
    16                              ld a,CONSOLE_STATUS_USB
    17                              and (hl)
    18                              jr nz,_readchar_usb
    19                              jr _readchar_end
    20                          _readchar_beanboard:
    21                              call key_readchar
    22                              jr _readchar_end
    23                          _readchar_usb:
    24                              call usb_readchar
    25                          _readchar_end:
    26                              pop hl
    27                              ret
    28                          ELSE
    29                          ; read a character from the console and return in A - return 0 if there is no character
    30                          readchar:
    31  000e  cd2900                call usb_readchar
    32  0011  c9                    ret
    33                          ENDIF
    34                          
    35                          IF BEANBOARD
    36                          ; sent character in A to the console
    37                          putchar:
    38                              push hl
    39                              push bc
    40                              ld b,a
    41                              ld hl,CONSOLE_STATUS
    42                              ld a,CONSOLE_STATUS_BEANBOARD
    43                              and (hl)
    44                              jr nz,_putchar_beanboard
    45                              ld a,CONSOLE_STATUS_USB
    46                              and (hl)
    47                              jr nz,_putchar_usb
    48                              jr _putchar_end
    49                          _putchar_beanboard:
    50                              ld a,b
    51                              call lcd_putchar
    52                              jr _putchar_end
    53                          _putchar_usb:
    54                              ld a,b
    55                              call usb_putchar
    56                          _putchar_end:
    57                              ld a,b
    58                              pop bc
    59                              pop hl
    60                              ret
    61                          ELSE
    62                          ; sent character in A to the console
    63                          putchar:
    64  0012  c5                    push bc
    65  0013  47                    ld b,a
    66  0014  cd3a00                call usb_putchar
    67  0017  78                    ld a,b
    68  0018  c1                    pop bc
    69  0019  c9                    ret
    70                          ENDIF
    71                          
    72                          ; print a zero-terminated string pointed to by hl to the console
    73                          puts:
    74  001a  e5                    push hl
    75                          _puts_loop:
    76                              ; get character from string
    77  001b  7e                    ld a,(hl)
    78                              ; is it zero?
    79  001c  fe00                  cp 0
    80                              ; yes
    81  001e  2807                  jr z, _puts_end
    82                              ; no: send character
    83  0020  cd1200                call putchar
    84                              ; next character position
    85  0023  23                    inc hl
    86                              ; loop for next character
    87  0024  c31b00                jp _puts_loop
    88                          _puts_end:
    89  0027  e1                    pop hl
    90  0028  c9                    ret
    91                          
    92                          IF BEANBOARD
    93                          ; determine which console should be active - Reset=beanboard, shift-Reset=USB
    94                          beanboard_console_init:
    95                              ; check for modifier keys being held down
    96                              call modifierkeys
    97                              ; shift key down?
    98                              and MOD_KEY_SHIFT
    99                              ; yes shift
   100                              jp nz,_beanboard_console_init_usb
   101                              ; no shift
   102                              ld a,CONSOLE_STATUS_BEANBOARD
   103                              ld hl,CONSOLE_STATUS
   104                              ld (hl),a
   105                              ret
   106                          _beanboard_console_init_usb:
   107                              ld a,CONSOLE_STATUS_USB
   108                              ld hl,CONSOLE_STATUS
   109                              ld (hl),a
   110                              ret
   111                          ENDIF
   112                          
beanzee.asm:
    11                          include "asm/UM245R.asm"
asm/UM245R.asm:
     1                          ; It is assumed that UM245R status signals are gated to the data bus as an IO port where:
     2                          ; /TXE = bit 0
     3                          ; /RXF = bit 1
     4                          ; As per:
     5                          ; https://github.com/PainfulDiodes/z80-breadboard-computer
     6                          ;
     7                          ; line endings are translated:
     8                          ; incoming line endings from the terminal are expected to be \r
     9                          ; and are tranlslated to \n
    10                          ; (\r\n would count as 2 line endings)
    11                          ; and outgoing line endings are sent as \r\n
    12                          ; externally this is consistent with VT100/ANSI terminal behaviour
    13                          ; and internally line endings are always \n
    14                          
    15                          ; get character and return in A
    16                          usb_readchar:
    17                              ; get the USB status
    18  0029  db00                  in a,(UM245R_CTRL)
    19                              ; data to read? (active low)
    20  002b  cb4f                  bit 1,a
    21                              ; no, the buffer is empty
    22  002d  2008                  jr nz,_usb_no_char
    23                              ; yes, read the received char
    24  002f  db01                  in a,(UM245R_DATA)
    25                              ; is CR?
    26  0031  fe0d                  cp ESC_R
    27                              ; no:
    28  0033  c0                    ret nz
    29                              ; yes: convert CR to LF
    30  0034  3e0a                  ld a, ESC_N
    31  0036  c9                    ret
    32                          _usb_no_char:
    33  0037  3e00                  ld a,0
    34  0039  c9                    ret
    35                          
    36                          usb_putchar:
    37                              ; newline?
    38  003a  fe0a                  cp ESC_N
    39                              ; no: just send the char
    40  003c  2007                  jr nz,_do_usb_put
    41  003e  3e0d                  ld a, ESC_R
    42  0040  cd4900                call _usb_put
    43  0043  3e0a                  ld a, ESC_N
    44                          _do_usb_put:
    45  0045  cd4900                call _usb_put
    46  0048  c9                    ret
    47                          
    48                          ; transmit character in A
    49                          _usb_put:
    50  0049  c5                    push bc
    51                              ; stash the transmit character
    52  004a  47                    ld b,a
    53                          _usb_put_loop:
    54                              ; get the USB status
    55  004b  db00                  in a,(UM245R_CTRL)
    56                              ; ready to transmit? (active low)
    57  004d  cb47                  bit 0,a
    58                              ; no: bit is high
    59  004f  20fa                  jr nz,_usb_put_loop
    60                              ; yes: restore the stashed transmit character
    61  0051  78                    ld a,b
    62                              ; transmit the character
    63  0052  d301                  out (UM245R_DATA),a
    64  0054  c1                    pop bc
    65  0055  c9                    ret
    66                          
    67                          ; print a zero-terminated string pointed to by hl to the USB
    68                          usb_puts:
    69  0056  e5                    push hl
    70                          _usb_puts_loop:
    71                              ; get character from string
    72  0057  7e                    ld a,(hl)
    73                              ; is it zero?
    74  0058  fe00                  cp 0
    75                              ; yes
    76  005a  2807                  jr z, _usb_puts_end
    77                              ; no: send character
    78  005c  cd3a00                call usb_putchar
    79                              ; next character position
    80  005f  23                    inc hl
    81                              ; loop for next character
    82  0060  c35700                jp _usb_puts_loop
    83                          _usb_puts_end:
    84  0063  e1                    pop hl
    85  0064  c9                    ret
    86                          
    87                          
beanzee.asm:
    12                          include "asm/marvin.asm"
asm/marvin.asm:
     1                          ; ****************************************************
     2                          ; *  Marvin - a Z80 homebrew monitor program
     3                          ; * (c) Stephen Willcock 2024
     4                          ; * https://github.com/PainfulDiodes
     5                          ; ****************************************************
     6                          
     7                          ; MAIN PROGRAM LOOP
     8                          
     9                          START:
    10                              ; point DE to zero - this is the default address argument for commands
    11  0065  110000                ld de,0x0000
    12                          
    13  0068  21ba01                ld hl,WELCOME_MSG
    14  006b  cd1a00                call puts
    15                          
    16                          PROMPT:
    17                              ; point HL to the beginning of the input buffer
    18  006e  2120f0                ld hl,CMD_BUFFER
    19  0071  3e3e                  ld a,'>'
    20  0073  cd1200                call putchar
    21                          
    22                          _get_cmd:
    23                              ; get character from console
    24  0076  cd0600                call getchar
    25                              ; echo the character to console
    26  0079  cd1200                call putchar
    27                              ; is CR?
    28  007c  fe0d                  cp ESC_R
    29                              ; yes: skip this
    30  007e  28f6                  jr z,_get_cmd
    31                              ; is tab?
    32  0080  fe09                  cp ESC_T
    33                              ; yes: skip this
    34  0082  28f2                  jr z,_get_cmd
    35                              ; is space?
    36  0084  fe20                  cp ' '
    37                              ; yes - skip this
    38  0086  28ee                  jr z,_get_cmd
    39                              ; escape?
    40  0088  fe1b                  cp ESC_E
    41                              ; yes
    42  008a  2808                  jr z, _get_cmd_esc
    43                              ; end of line?
    44  008c  fe0a                  cp ESC_N
    45                              ; yes
    46  008e  280b                  jr z, _get_cmd_end
    47                              ; no: add character to the buffer
    48  0090  77                    ld(hl),a
    49                              ; move pointer to next buffer location - we're not checking for overrun
    50  0091  23                    inc hl
    51                              ; next character
    52  0092  18e2                  jr _get_cmd
    53                              ; do escape
    54                          _get_cmd_esc:
    55                              ; new line
    56  0094  3e0a                  ld a,ESC_N
    57  0096  cd1200                call putchar
    58                              ; back to prompt
    59  0099  18d3                  jr PROMPT
    60                          _get_cmd_end:
    61                              ; string terminator
    62  009b  3e00                  ld a,0
    63                              ; add terminator to end of buffer
    64  009d  77                    ld(hl),a
    65                          ; process command from buffer
    66                              ; point to start of buffer
    67  009e  2120f0                ld hl,CMD_BUFFER
    68                              ; load character from buffer
    69  00a1  7e                    ld a,(hl)
    70                              ; end of string?
    71  00a2  fe00                  cp 0
    72                              ; yes - empty line - go back to prompt
    73  00a4  28c8                  jr z,PROMPT
    74                              ; advance the buffer pointer
    75  00a6  23                    inc hl
    76  00a7  fe72                  cp 'r'
    77  00a9  2815                  jr z,_cmd_read
    78  00ab  fe77                  cp 'w'
    79  00ad  2847                  jr z,_cmd_write
    80  00af  fe78                  cp 'x'
    81  00b1  2868                  jr z,_cmd_execute
    82                              ; ':' = load from intel hex format
    83  00b3  fe3a                  cp ':'
    84  00b5  2879                  jr z,_cmd_load
    85                              ; otherwise error
    86  00b7  212902                ld hl,BAD_CMD_MSG
    87  00ba  cd1a00                call puts
    88                              ; loop back to the prompt
    89  00bd  c36e00                jp PROMPT
    90                          
    91                          ; COMMANDS
    92                          
    93                          ; READ
    94                          ; read bytes from memory and send hex values to console
    95                          _cmd_read:
    96                              ; load character from buffer
    97  00c0  7e                    ld a,(hl)
    98                              ; end of string?
    99  00c1  fe00                  cp 0
   100                              ; yes: no address argument, so skip to read row
   101  00c3  2808                  jr z, _cmd_read_row
   102                              ; parse first pair of characters
   103  00c5  cd5f01                call hex_byte_val
   104                              ; load into upper byte of memory pointer
   105  00c8  57                    ld d,a
   106                              ; parse second pair of characters
   107  00c9  cd5f01                call hex_byte_val
   108                              ; load into lower byte of memory pointer
   109  00cc  5f                    ld e,a
   110                          _cmd_read_row:
   111                              ; initialise byte counter - each row will have this many bytes
   112  00cd  0e10                  ld c, 0x10
   113                              ; print DE content: the read address
   114  00cf  7a                    ld a,d
   115  00d0  cd9301                call putchar_hex
   116  00d3  7b                    ld a,e
   117  00d4  cd9301                call putchar_hex
   118                              ; separator between address and data
   119  00d7  3e3a                  ld a,':'
   120  00d9  cd1200                call putchar
   121  00dc  3e20                  ld a,' '
   122  00de  cd1200                call putchar
   123                              ; get a byte
   124                          _cmd_read_byte:
   125  00e1  1a                    ld a,(de)
   126                              ; and print it
   127  00e2  cd9301                call putchar_hex
   128                              ; add space between bytes
   129  00e5  3e20                  ld a,' '
   130  00e7  cd1200                call putchar
   131                              ; next address
   132  00ea  13                    inc de
   133                              ; reduce byte counter
   134                              ; TODO djnz ?
   135  00eb  0d                    dec c
   136                              ; repeat if the counter is not 0
   137  00ec  20f3                  jr nz, _cmd_read_byte
   138                              ; otherwise, new line
   139  00ee  3e0a                  ld a,ESC_N
   140  00f0  cd1200                call putchar
   141                              ; and back to prompt
   142  00f3  c36e00                jp PROMPT
   143                          
   144                          ; WRITE
   145                          
   146                          ; write bytes to memory interpreting hex values from console
   147                          _cmd_write:
   148                              ; load character from buffer
   149  00f6  7e                    ld a,(hl)
   150                              ; end of string?
   151  00f7  fe00                  cp 0
   152                              ; yes: no data
   153  00f9  2817                  jr z, _cmd_write_null
   154                              ; parse first pair of characters - address high
   155  00fb  cd5f01                call hex_byte_val
   156                              ; load into upper byte of memory pointer
   157  00fe  57                    ld d,a
   158                              ; parse second pair of characters - address low
   159  00ff  cd5f01                call hex_byte_val
   160                              ; load into lower byte of memory pointer
   161  0102  5f                    ld e,a
   162                          _cmd_write_data:
   163                              ; load character from buffer
   164  0103  7e                    ld a,(hl)
   165                              ; end of string?
   166  0104  fe00                  cp 0
   167                              ; yes: we're done
   168  0106  2807                  jr z, _cmd_write_end
   169                              ; parse data byte
   170  0108  cd5f01                call hex_byte_val
   171                              ; write byte to memory
   172  010b  12                    ld (de),a
   173                              ; advance destination pointer
   174  010c  13                    inc de
   175  010d  18f4                  jr _cmd_write_data
   176                          _cmd_write_end:
   177  010f  c36e00                jp PROMPT
   178                              ; w with no data
   179                          _cmd_write_null:
   180  0112  214102                ld hl,CMD_W_NULL_MSG
   181  0115  cd1a00                call puts
   182                              ; and back to prompt
   183  0118  c36e00                jp PROMPT
   184                          
   185                          ; EXECUTE
   186                          
   187                          ; start executing from given address
   188                          _cmd_execute:
   189                              ; load character from buffer
   190  011b  7e                    ld a,(hl)
   191                              ; end of string?
   192  011c  fe00                  cp 0
   193                              ; yes - no data
   194  011e  ca2c01                jp z, _cmd_exec_df
   195                              ; parse first pair of characters - address high
   196  0121  cd5f01                call hex_byte_val
   197                              ; load into upper byte of memory pointer
   198  0124  57                    ld d,a
   199                              ; parse second pair of characters - address low
   200  0125  cd5f01                call hex_byte_val
   201                              ; load into lower byte of memory pointer
   202  0128  5f                    ld e,a
   203  0129  626b                  ld hl,de
   204                              ; execute from address
   205  012b  e9                    jp (hl)
   206                              ; start executing from default address
   207                          _cmd_exec_df:
   208  012c  210080                ld hl,RAMSTART
   209                              ; execute from address
   210  012f  e9                    jp (hl)
   211                          
   212                          ; LOAD
   213                          
   214                          ; load from INTEL HEX - records are read from the buffer
   215                          _cmd_load:
   216                              ; load character from buffer
   217  0130  7e                    ld a,(hl)
   218                              ; end of string?
   219  0131  fe00                  cp 0
   220                              ; yes: no data - quit
   221  0133  ca5c01                jp z, _cmd_load_end
   222                              ; parse first pair of characters - byte count
   223  0136  cd5f01                call hex_byte_val
   224  0139  fe00                  cp 0
   225                              ; yes - zero byte count - quit
   226  013b  ca5c01                jp z, _cmd_load_end
   227                              ; load byte count into C
   228  013e  4f                    ld c,a
   229                              ; parse address high
   230  013f  cd5f01                call hex_byte_val
   231                              ; load into upper byte of memory pointer
   232  0142  57                    ld d,a
   233                              ; parse address low
   234  0143  cd5f01                call hex_byte_val
   235                              ; load into lower byte of memory pointer
   236  0146  5f                    ld e,a
   237                              ; parse record type
   238  0147  cd5f01                call hex_byte_val
   239                              ; record type zero?
   240  014a  fe00                  cp 0
   241                              ; no: quit
   242  014c  c25c01                jp nz, _cmd_load_end
   243                          _cmd_load_data:
   244                              ; load character from buffer
   245  014f  7e                    ld a,(hl)
   246                              ; end of string?
   247  0150  fe00                  cp 0
   248                              ; yes: we're done
   249  0152  2808                  jr z, _cmd_load_end
   250                              ; no:
   251                              ; parse data byte
   252  0154  cd5f01                call hex_byte_val
   253                              ; write byte to memory
   254  0157  12                    ld (de),a
   255                              ; advance destination pointer
   256  0158  13                    inc de
   257                              ; decrement byte counter
   258                              ; TODO djnz
   259  0159  0d                    dec c
   260                              ; if byte counter not zero then go again
   261  015a  20f3                  jr nz,_cmd_load_data
   262                          _cmd_load_end:
   263  015c  c36e00                jp PROMPT
   264                          
beanzee.asm:
    13                          include "asm/strings.asm"
asm/strings.asm:
     1                          ; string subroutines
     2                          
     3                          ; read 2 ASCII hex chars from memory by HL pointer, return converted value in A and advance HL pointer
     4                          hex_byte_val:
     5                              ; preserve BC
     6  015f  c5                    push bc
     7                              ; load 1st character from memory
     8  0160  7e                    ld a,(hl)
     9                              ; end of string?
    10  0161  fe00                  cp 0
    11                              ; yes: no value - return zero
    12  0163  2819                  jr z,_hex_byte_val_zero
    13                              ; no:
    14                              ; advance the buffer pointer
    15  0165  23                    inc hl
    16                              ; convert first hex digit
    17  0166  cd8201                call hex_val
    18                              ; shift left 4 bits to put value into top nibble
    19  0169  cb27                  sla a
    20  016b  cb27                  sla a
    21  016d  cb27                  sla a
    22  016f  cb27                  sla a
    23                              ; cache the result
    24  0171  47                    ld b,a
    25                              ; load 2nd character from memory
    26  0172  7e                    ld a,(hl)
    27                              ; end of string?
    28  0173  fe00                  cp 0
    29                              ; yes: incomplete byte - return zero
    30  0175  2807                  jr z,_hex_byte_val_zero
    31                              ; advance the buffer pointer
    32  0177  23                    inc hl
    33                              ; and convert 2nd hex digit
    34  0178  cd8201                call hex_val
    35                              ; add first and second digits
    36  017b  80                    add a,b
    37                              ; restore BC
    38  017c  c1                    pop bc
    39  017d  c9                    ret
    40                          _hex_byte_val_zero:
    41                              ; zero return value
    42  017e  3e00                  ld a,0
    43                              ; restore BC
    44  0180  c1                    pop bc
    45  0181  c9                    ret
    46                          
    47                          ; convert an ASCII hex char in A to a number value (lower 4 bits)
    48                          hex_val:
    49                              ; is it lowercase alphabetic?
    50  0182  fe61                  cp 'a'
    51                              ; no: uppercase/numeric
    52  0184  3803                  jr c,_hex_val_u_n
    53                              ; yes: alphabetic
    54  0186  d657                  sub 'a'-0x0a
    55  0188  c9                    ret
    56                          _hex_val_u_n:
    57                              ; is it uppercase alphabetic?
    58  0189  fe41                  cp 'A'
    59                              ; no: numeric
    60  018b  3803                  jr c,_hex_val_n
    61                              ; y:
    62  018d  d637                  sub 'A'-0x0a
    63  018f  c9                    ret
    64                          _hex_val_n:
    65                              ; numeric
    66  0190  d630                  sub '0'
    67  0192  c9                    ret
    68                          
    69                          ; convert value in A into an ASCII pair and send to console
    70                          putchar_hex:
    71  0193  f5                    push af
    72  0194  c5                    push bc
    73                              ; stash in B
    74  0195  47                    ld b,a
    75                              ; shift A right x4 e.g. transform 10110010 to 00001011
    76  0196  cb3f                  srl a
    77  0198  cb3f                  srl a
    78  019a  cb3f                  srl a
    79  019c  cb3f                  srl a
    80                              ; most significant digit
    81  019e  cdaa01                call _putchar_hex_dgt
    82                              ; recover from stash
    83  01a1  78                    ld a,b
    84                              ; clear the top 4 bits
    85  01a2  e60f                  and 0b00001111
    86                              ; least significant digit
    87  01a4  cdaa01                call _putchar_hex_dgt
    88  01a7  c1                    pop bc
    89  01a8  f1                    pop af
    90  01a9  c9                    ret
    91                          _putchar_hex_dgt:
    92                              ; is it an alpha or numeric?
    93  01aa  fe0a                  cp 0x0a
    94                              ; numeric
    95  01ac  3806                  jr c,_putchar_hex_n
    96                              ; alpha
    97                              ; for alpha add the base ascii for 'a' but then sub 10 / 0x0a as hex 'a' = 10d
    98  01ae  c657                  add a,'a'-0x0a
    99  01b0  cd1200                call putchar
   100  01b3  c9                    ret
   101                          _putchar_hex_n:
   102                              ; for numeric add the base ascii for '0'
   103  01b4  c630                  add a,'0'
   104  01b6  cd1200                call putchar
   105  01b9  c9                    ret
   106                          
